<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainScreen.HomeScreen.HomeFragment">


    <Button
        android:id="@+id/add"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:background="@color/purple"
        android:text="Add Event"
        android:textColor="#FFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent" />

    <EditText
        android:outlineSpotShadowColor="#77000000"
        android:shadowColor="#77000000"
        android:id="@+id/editTextTextPersonName"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:hint="Event Name"
        android:inputType="textPersonName"
        android:outlineAmbientShadowColor="#77000000"
        android:textColor="#000000"
        android:textColorHint="#77000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <EditText
        android:outlineSpotShadowColor="#77000000"
        android:shadowColor="#77000000"
        android:outlineAmbientShadowColor="#77000000"
        android:id="@+id/editTextTextPersonName2"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:hint="Event Date"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#77000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName" />

    <EditText
        android:outlineSpotShadowColor="#77000000"
        android:shadowColor="#77000000"
        android:outlineAmbientShadowColor="#77000000"
        android:id="@+id/editTextTextPersonName3"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:hint="Event Time"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#77000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName2" />

    <EditText
        android:outlineSpotShadowColor="#77000000"
        android:shadowColor="#77000000"
        android:outlineAmbientShadowColor="#77000000"
        android:id="@+id/editTextTextPersonName4"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:hint="Event Location"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#77000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName3" />

    <EditText
        android:outlineSpotShadowColor="#77000000"
        android:shadowColor="#77000000"
        android:outlineAmbientShadowColor="#77000000"
        android:id="@+id/editTextTextPersonName5"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:hint="Team One"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#77000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName4" />

    <EditText
        android:outlineSpotShadowColor="#77000000"
        android:shadowColor="#77000000"
        android:outlineAmbientShadowColor="#77000000"
        android:id="@+id/editTextTextPersonName6"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:hint="Team Two"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#77000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName5" />

    <EditText
        android:outlineSpotShadowColor="#77000000"
        android:shadowColor="#77000000"
        android:outlineAmbientShadowColor="#77000000"
        android:id="@+id/editTextTextPersonName7"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:hint="Event Image Link"
        android:inputType="textPersonName"
        android:textColor="#000000"
        android:textColorHint="#77000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.5"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName6" />

    <EditText
        android:id="@+id/editTextTextPersonName8"
        android:layout_width="300dp"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:ems="10"
        android:hint="Event Price"
        android:inputType="textPersonName"
        android:outlineAmbientShadowColor="#77000000"
        android:outlineSpotShadowColor="#77000000"
        android:shadowColor="#77000000"
        android:textColor="#000000"
        android:textColorHint="#77000000"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.504"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/editTextTextPersonName7" />

</androidx.constraintlayout.widget.ConstraintLayout>